    <frontend>
        <routers>
            <onestepcheckout>
                <args>
                    <modules>
                        <Innoexts_WarehouseOneStepCheckout before="Idev_OneStepCheckout">Innoexts_WarehouseOneStepCheckout_Onestepcheckout</Innoexts_WarehouseOneStepCheckout>
